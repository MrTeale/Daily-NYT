{
  "name": "daily-nyt",
  "version": "1.0.0",
  "description": "Daily NYT front page fetcher using AWS SAM",
  "scripts": {
    "validate": "sam validate --lint",
    "build": "sam build",
    "build:container": "sam build --use-container",
    "deploy": "sam deploy",
    "deploy:prod": "sam deploy --config-env prod",
    "deploy:guided": "sam deploy --guided",
    "local:api": "sam local start-api",
    "local:invoke": "sam local invoke MainFunction",
    "logs": "sam logs -n MainFunction --stack-name daily-nyt-prod --tail",
    "delete": "sam delete --stack-name daily-nyt-prod"
  },
  "keywords": ["aws", "sam", "serverless", "lambda", "nyt"],
  "author": "MrTeale",
  "license": "MIT"
}
